@model Yogam.AMC.Web.Models.UserRegistrationData
<div class="row" style="background-image:@Url.Content("~/Content/images/YogamLogo_Transparant.png")">
    <div class="col-md-7">
        <div class="row">
            <div class="col-md-12" style="padding-right: 0;">
                <div id="rcorners1" style="margin-bottom: 2px; text-align: left; background-color: #ea5b0c; background-image:url(http://www.yogamhealth.com/Content/images/YogamLogo_Transparant.png)">
                    <div class="row">
                        <div class="col-md-offset-6 col-md-6">

                            <h3>
                                Yogam Health Care
                            </h3>

                        </div>
                        <br />
                        <div id="info" class="col-xs-7">

                            <p>Patient Fist Name : @Model.Fistname</p>
                            <p>Patient Last Name : @Model.Lastname</p>
                            <p>Patient Country : @Model.Country</p>
                            <p>Patient Confidential ID: @Model.YogamCardNumber</p>
                            <p>Valid From: @Model.ValidFrom</p>
                            <p>Expired On : @Model.ValidTo</p>

                        </div>
                        <div class="col-md-1">

                        </div>
                        <div class="col-md-4" style="float:right;">
                            <p style="display:inline;vertical-align:bottom;">www.yogamhealth.com</p>
                            <img height="100px" width="120px"
                                 src="@Server.MapPath("~/Content/images/Yogam_Rfid.png")" class="img-responsive center-block">

                        </div>

                    </div>

                    <div class="row">
                        <div class="col-xs-9"></div>
                        <div class="col-xs-3">
                            <img height="60px" width="150px"
                                 src="@Server.MapPath("~/Content/images/YogamBarCode.jpg")"
                                 style="float: right;" class="img-responsive center-block">
                        </div>

                    </div>


                </div>
            </div>

        </div>
    </div>

</div>



